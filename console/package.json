    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^2.1.1",
    "keycloak-js": "^21.0.1",
    "loglevel": "^1.8.1",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-app-polyfill": "^3.0.0",
    "react-cookies": "^0.1.1",
    "react-dom": "^18.2.0",
    "react-fast-marquee": "^1.3.5",
    "react-i18next": "^12.2.0",
