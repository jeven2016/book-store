    "web-vitals": "^3.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@craco/craco": "^7.0.0",
    "circular-dependency-plugin": "^5.2.2",
    "commitizen": "^4.3.0",
    "conventional-changelog-cli": "^2.2.2",
    "craco-plugin-react-hot-reload": "^0.1.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "@typescript-eslint/eslint-plugin": "5.54.1",
    "@typescript-eslint/parser": "5.54.1",
    "eslint": "8.35.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.7.0",
    "eslint-config-standard": "17.0.0",
    "eslint-config-standard-react": "13.0.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "lodash": "^4.17.21",
    "prettier": "^2.8.4",
    "react-hot-loader": "^4.13.1",
    "sass": "^1.58.3",
    "serve": "^14.2.0",
    "simple-progress-webpack-plugin": "^2.0.0",
    "webpack-bundle-analyzer": "^4.8.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development craco start ",
    "build": "cross-env NODE_ENV=production craco build",
    "preview": "serve -s build",
    "test": "cross-env NODE_ENV=development craco test",
    "eject": "react-scripts eject",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,scss,md,json}' --config ./.prettierrc",
    "prepare": "husky install",
    "genlog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "commit": "git-cz"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "src/**/*.{js,jsx,ts,tsx,css,scss,md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
